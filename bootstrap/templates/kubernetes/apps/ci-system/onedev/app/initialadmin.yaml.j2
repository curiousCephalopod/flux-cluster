---
apiVersion: external-secrets.io/v1beta1
kind: ExternalSecret
metadata:
  name: onedev-admin-secret
spec:
  secretStoreRef:
    kind: ClusterSecretStore
    name: onepassword-connect
  target:
    creationPolicy: Owner
  data:
  - secretKey: password
    remoteRef:
      key: onedev-admin
      property: password
